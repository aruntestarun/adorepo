pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                snDevOpsStep()
                echo 'Compiling and Building..'
                sh 'printenv'
            }
        }
        stage('Test') {
            steps {
               snDevOpsStep()
                echo 'Testing...'
                sleep 3
            }
        }
        stage('Deploy') {
            steps {
               snDevOpsStep()
               snDevOpsChange()
                echo 'Deploying...'
                sleep 4
            }
        }
    }
}
