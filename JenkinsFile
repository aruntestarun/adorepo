pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                snDevOpsStep()
                echo 'Building..'
                sh 'printenv'
            }
        }
        stage('Test') {
            steps {
               snDevOpsStep()
                echo 'Testing..'
                sleep 3
            }
        }
        stage('Deploy') {
            steps {
               snDevOpsStep()
                echo 'Deploying....'
                sleep 4
            }
        }
    }
}
